if ! getent group hiawatha | grep -q ^hiawatha: 2>/dev/null ; then
  groupadd -g 259 hiawatha
fi

if ! getent passwd hiawatha | grep -q ^hiawatha: 2>/dev/null ; then
  useradd -u 259 -g 259 -c "User for hiawatha" -d / -s /bin/false hiawatha
fi

if ! getent group php-fcgi | grep -q ^php-fcgi: 2>/dev/null ; then
  groupadd -g 260 php-fcgi
fi

if ! getent passwd php-fcgi | grep -q ^php-fcgi: 2>/dev/null ; then
  useradd -u 260 -g 260 -c "User for php-fcgi" -d / -s /bin/false php-fcgi
fi
